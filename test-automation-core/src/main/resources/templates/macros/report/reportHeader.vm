#macro(includeReportHeader, $table_key, $parallel)

<thead>
  <tr class="header dont-sort">
    #if ($parallel)
      <th></th>
    #end
    <th></th>
    <th colspan="3">Scenarios</th>
    <th colspan="2">Features</th>
  </tr>
  <tr>
    <th>$table_key</th>
    #if ($parallel)
      <th>Device</th>
    #end

    <th class="passed">Passed</th>
    <th class="failed">Failed</th>
    <th class="total">Total</th>

    <th>Duration</th>
    <th>Status</th>
  </tr>
</thead>

#end
